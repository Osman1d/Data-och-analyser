# Ignorera alla bildfiler
*.jpg
*.jpeg
*.png

# Ignorera datasets
original_data/
experiment_small_data/
experiment_tiny_data/
import os
import random
import matplotlib.pyplot as plt
import matplotlib.image as mpimg

# Definiera sökvägar
train_dir = 'original_data/train/train'

# Läs in bildfiler
all_images = os.listdir(train_dir)
sample_images = random.sample(all_images, 10)

# Plotta bilder och extrahera labels
plt.figure(figsize=(15, 10))
for i, img_name in enumerate(sample_images):
    img_path = os.path.join(train_dir, img_name)
    img = mpimg.imread(img_path)
    label = 'Dog' if 'dog' in img_name else 'Cat'
    plt.subplot(2, 5, i + 1)
    plt.imshow(img)
    plt.title(label)
    plt.axis('off')
plt.show()import os

# Definiera folderstruktur
folders = [
    'experiment_small_data/test',
    'experiment_small_data/train',
    'experiment_small_data/val',
    'experiment_tiny_data/test',
    'experiment_tiny_data/train',
    'experiment_tiny_data/val',
    'original_data/test/test',
    'original_data/train/train'
]

# Skapa foldrarna
for folder in folders:
    os.makedirs(folder, exist_ok=True)